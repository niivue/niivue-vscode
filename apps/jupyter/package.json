{
  "name": "@niivue/jupyter",
  "version": "0.1.2",
  "description": "A JupyterLab extension for viewing NIfTI files with Niivue",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "niivue",
    "nifti",
    "neuroimaging"
  ],
  "homepage": "https://github.com/niivue/jupyterlab-niivue",
  "bugs": {
    "url": "https://github.com/niivue/jupyterlab-niivue/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Korbinian Eckstein",
    "email": "korbinian90@gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "static/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/niivue/jupyterlab-niivue.git"
  },
  "scripts": {
    "build": "pnpm copy:assets && pnpm build:lib && pnpm build:labextension:dev",
    "build:prod": "pnpm clean && pnpm copy:assets && pnpm build:lib:prod && pnpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
  "build:css": "pnpm exec postcss style/index.css -o static/niivue/index.css",
  "copy:assets": "pnpm build:css && cd ../../packages/niivue-react && BUILD_TARGET=vscode pnpm build && mkdir -p ../../apps/jupyter/static/niivue && cp -r dist/* ../../apps/jupyter/static/niivue/",
  "clean": "pnpm clean:lib",
  "clean:lib": "rimraf lib tsconfig.tsbuildinfo .tsbuildinfo .webpack-cache",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf jupyterlab_niivue/labextension jupyterlab_niivue/_version.py",
    "clean:all": "pnpm clean:lib && pnpm clean:labextension && pnpm clean:lintcache",
    "eslint": "npx eslint . --cache --ext .ts,.tsx --fix",
    "eslint:check": "npx eslint . --cache --ext .ts,.tsx",
    "install:extension": "pnpm build",
    "lint": "pnpm stylelint && pnpm prettier && pnpm eslint",
    "lint:check": "pnpm stylelint:check && pnpm prettier:check && pnpm eslint:check",
    "prettier": "npx prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\" --write --list-different",
    "prettier:base": "npx prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "pnpm prettier:base --check",
    "stylelint": "npx stylelint --cache \"style/**/*.css\" --fix",
    "stylelint:check": "npx stylelint --cache \"style/**/*.css\"",
    "test": "echo \"No tests specified\"",
    "dev": "run-p watch:src watch:labextension",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w --sourceMap --preserveWatchOutput",
    "watch:labextension": "jupyter labextension watch . --development=True"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.4.5",
    "@jupyterlab/apputils": "^4.5.5",
    "@jupyterlab/docregistry": "^4.4.5",
    "@jupyterlab/fileeditor": "^4.4.5",
    "@jupyterlab/services": "^7.4.5",
    "@lumino/widgets": "^2.7.1",
    "@niivue/niivue": "^0.62.1",
    "@niivue/react": "workspace:*"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.4.5",
    "@niivue/dicom-loader": "^1.0.0",
    "@niivue/pwa": "workspace:*",
    "@preact/signals": "^2.0.1",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "eslint-plugin-prettier": "^5.5.3",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "preact": "^10.24.1",
    "rimraf": "^6.0.1",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.23.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript-eslint": "^8.38.0",
    "yjs": "^13.6.27",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_niivue/labextension",
    "schemaDir": "schema",
    "webpackConfig": "./webpack.config.js"
  },
  "jupyter-releaser": {
    "skip": [
      "check-links"
    ]
  }
}