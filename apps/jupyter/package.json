{
  "name": "@niivue/jupyter",
  "version": "0.1.2",
  "description": "A JupyterLab extension for viewing NIfTI files with Niivue",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "niivue",
    "nifti",
    "neuroimaging"
  ],
  "homepage": "https://github.com/niivue/jupyterlab-niivue",
  "bugs": {
    "url": "https://github.com/niivue/jupyterlab-niivue/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Korbinian Eckstein",
    "email": "korbinian90@gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "static/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/niivue/jupyterlab-niivue.git"
  },
  "scripts": {
    "build": "pnpm run build:assets && pnpm run build:lib && pnpm run build:labextension:dev",
    "build:prod": "pnpm run clean && pnpm run build:assets && pnpm run build:lib:prod && pnpm run build:labextension",
    "build:assets": "cd ../../packages/niivue-react && BUILD_TARGET=vscode pnpm build && mkdir -p ../../apps/jupyter/static/niivue && cp -r dist/* ../../apps/jupyter/static/niivue/",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "clean": "rimraf lib tsconfig.tsbuildinfo .tsbuildinfo .webpack-cache jupyterlab_niivue/labextension jupyterlab_niivue/_version.py .eslintcache .stylelintcache",
    "dev": "run-p watch:lib watch:labextension",
    "watch": "pnpm run dev",
    "watch:lib": "tsc -w --sourceMap --preserveWatchOutput",
    "watch:labextension": "jupyter labextension watch . --development=True",
    "lint": "pnpm run lint:eslint && pnpm run lint:prettier && pnpm run lint:style",
    "lint:fix": "pnpm run lint:eslint --fix && pnpm run lint:prettier --write && pnpm run lint:style --fix",
    "lint:eslint": "eslint . --cache --ext .ts,.tsx",
    "lint:prettier": "prettier \"**/*.{ts,tsx,js,jsx,css,json,md}\" --check",
    "lint:style": "stylelint --cache \"style/**/*.css\"",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,css,json,md}\" --write",
    "test": "echo \"Error: no tests specified\" && exit 0"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.4.5",
    "@jupyterlab/apputils": "^4.5.5",
    "@jupyterlab/docregistry": "^4.4.5",
    "@jupyterlab/fileeditor": "^4.4.5",
    "@jupyterlab/services": "^7.4.5",
    "@lumino/widgets": "^2.7.1",
    "@niivue/niivue": "^0.62.1",
    "@niivue/react": "workspace:*"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.4.5",
    "@niivue/dicom-loader": "^1.0.0",
    "@niivue/pwa": "workspace:*",
    "@preact/signals": "^2.0.1",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "eslint-plugin-prettier": "^5.5.3",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "preact": "^10.24.1",
    "rimraf": "^6.0.1",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.23.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript-eslint": "^8.38.0",
    "yjs": "^13.6.27"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_niivue/labextension",
    "schemaDir": "schema",
    "webpackConfig": "./webpack.config.js"
  },
  "jupyter-releaser": {
    "skip": [
      "check-links"
    ]
  }
}
