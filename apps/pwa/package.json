{
  "name": "@niivue/pwa",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "pnpm run type-check && vite build",
    "build:analyze": "pnpm run build && npx vite-bundle-analyzer",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "playwright test",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "pnpm run lint -- --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,css,md}",
    "watch": "vite build --watch",
    "ci": "pnpm run lint && pnpm run type-check && pnpm run test:coverage && pnpm run test:e2e",
    "clean": "rm -rf dist build"
  },
  "dependencies": {
    "@niivue/dicom-loader": "catalog:",
    "@niivue/niivue": "catalog:",
    "@niivue/react": "workspace:*",
    "@preact/signals": "catalog:",
    "preact": "catalog:",
    "react-router-dom": "catalog:"
  },
  "devDependencies": {
    "@babel/core": "catalog:",
    "@playwright/test": "catalog:",
    "@preact/preset-vite": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/preact": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitest/ui": "^3.0.6",
    "autoprefixer": "catalog:",
    "husky": "catalog:",
    "jsdom": "^26.0.0",
    "less": "catalog:",
    "lint-staged": "catalog:",
    "postcss": "catalog:",
    "tailwindcss": "catalog:",
    "vite-plugin-pwa": "catalog:",
    "vitest": "^3.0.6",
    "workbox-build": "catalog:",
    "workbox-window": "^7.0.0"
  }
}