{
  "name": "@niivue/pwa",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "node copy-assets.mjs && pnpm run type-check && vite build",
    "build:analyze": "pnpm run build && npx vite-bundle-analyzer",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "pnpm run lint -- --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,css,md}",
    "watch": "vite build --watch",
    "ci": "pnpm run lint && pnpm run type-check && pnpm run test:coverage && pnpm run test:e2e",
    "clean": "rm -rf dist build"
  },
  "dependencies": {
    "@niivue/dicom-loader": "^1.0.0",
    "@niivue/niivue": "^0.61.0",
    "@niivue/react": "workspace:*",
    "@preact/signals": "^2.2.1",
    "preact": "^10.27.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@niivue/dcm2niix": "^1.0.0",
    "@playwright/test": "^1.54.2",
    "@preact/preset-vite": "^2.10.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/preact": "^3.2.4",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "less": "^4.4.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-plugin-pwa": "^1.0.2",
    "vite-plugin-virtual": "^0.4.0",
    "vitest": "^3.2.4",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  }
}
