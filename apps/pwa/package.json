{
  "name": "@niivue/pwa",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "pnpm run type-check && vite build",
    "build:analyze": "pnpm run build && npx vite-bundle-analyzer",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "playwright test",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "pnpm run lint -- --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,css,md}",
    "watch": "vite build --watch",
    "ci": "pnpm run lint && pnpm run type-check && pnpm run test:coverage && pnpm run test:e2e",
    "clean": "rm -rf dist build"
  },
  "dependencies": {
    "@niivue/dicom-loader": "^1.0.0",
    "@niivue/niivue": "^0.61.0",
    "@niivue/react": "workspace:*",
    "@preact/signals": "^2.0.1",
    "preact": "^10.24.1",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@playwright/test": "^1.47.2",
    "@preact/preset-vite": "^2.9.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/preact": "^3.2.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/ui": "^3.0.6",
    "autoprefixer": "^10.4.18",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "less": "^4.1.3",
    "lint-staged": "^16.1.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.0.6",
    "workbox-build": "^7.0.0",
    "workbox-window": "^7.0.0"
  }
}