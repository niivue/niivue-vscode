{
  "name": "niivue",
  "displayName": "NiiVue",
  "description": "View NIfTI images (and a lot of other medical imaging formats)",
  "version": "1.0.14",
  "publisher": "KorbinianEckstein",
  "icon": "niivue_icon.png",
  "bugs": {
    "url": "https://github.com/niivue/niivue-vscode/issues",
    "email": "korbinian90+niivue@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niivue/niivue-vscode"
  },
  "homepage": "https://github.com/niivue",
  "engines": {
    "vscode": "^1.70.0"
  },
  "license": "MIT",
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "MRI",
    "NIfTI",
    "DICOM",
    "imaging",
    "medical",
    "neuroimaging",
    "neuroscience",
    "neuro",
    "brain",
    "fMRI",
    "DTI",
    "viewer"
  ],
  "extensionKind": [
    "workspace"
  ],
  "main": "./dist/extension-vscode.js",
  "browser": "./dist/extension-web.js",
  "contributes": {
    "commands": [
      {
        "command": "niivue.openWebLink",
        "title": "NiiVue: Open Web Link"
      },
      {
        "command": "niiVue.compareFromExplorer",
        "title": "NiiVue: Compare",
        "enablement": "listMultiSelection"
      },
      {
        "command": "niiVue.openFromExplorer",
        "title": "NiiVue: Open",
        "enablement": "!listMultiSelection"
      }
    ],
    "customEditors": [
      {
        "viewType": "niiVue.default",
        "displayName": "NiiVue",
        "selector": [
          {
            "filenamePattern": "*.dcm"
          },
          {
            "filenamePattern": "*.nii"
          },
          {
            "filenamePattern": "*.nii.gz"
          },
          {
            "filenamePattern": "*.mih"
          },
          {
            "filenamePattern": "*.mif"
          },
          {
            "filenamePattern": "*.mif.gz"
          },
          {
            "filenamePattern": "*.nhdr"
          },
          {
            "filenamePattern": "*.nrrd"
          },
          {
            "filenamePattern": "*.mhd"
          },
          {
            "filenamePattern": "*.mha"
          },
          {
            "filenamePattern": "*.mgh"
          },
          {
            "filenamePattern": "*.mgz"
          },
          {
            "filenamePattern": "*.v"
          },
          {
            "filenamePattern": "*.v16"
          },
          {
            "filenamePattern": "*.vmr"
          },
          {
            "filenamePattern": "*.mz3"
          },
          {
            "filenamePattern": "*.gii"
          }
        ]
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "niiVue.compareFromExplorer",
          "when": "listMultiSelection"
        },
        {
          "command": "niiVue.openFromExplorer",
          "when": "!listMultiSelection"
        }
      ]
    }
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack --mode production",
    "watch": "webpack --watch --mode development",
    "package": "webpack --mode production --devtool hidden-source-map",
    "webpack:dev": "webpack --mode development watch",
    "webpack:prod": "webpack --mode production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@niivue/niivue": "^0.38.0",
    "@preact/signals": "^1.2.1",
    "@types/glob": "^8.1.0",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "glob": "^8.1.0",
    "htm": "^3.1.1",
    "preact": "^10.18.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {}
}
