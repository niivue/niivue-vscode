{
  "name": "@niivue/monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "NiiVue monorepo with multiple packages and applications",
  "homepage": "https://github.com/niivue/niivue-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/niivue/niivue-vscode"
  },
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "dev": "turbo dev --parallel",
    "dev:source": "concurrently \"pnpm --filter @niivue/react dev:types-only\" \"pnpm --filter @niivue/pwa dev\"",
    "clean": "turbo clean && rm -rf node_modules packages/*/node_modules apps/*/node_modules",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "watch": "turbo watch"
  },
  "devDependencies": {
    "@preact/preset-vite": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@vitejs/plugin-legacy": "catalog:",
    "concurrently": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "prettier": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
